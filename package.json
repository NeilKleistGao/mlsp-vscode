{
  "name": "mlsp",
  "displayName": "mlsp",
  "description": "mlscript lsp",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NeilKleistGao/mlsp-vscode"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mlscript",
      "aliases": ["mlscript", "mlscript"],
      "extensions": [".mls"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "mlscript",
      "scopeName": "source.mls",
      "path": "./syntaxes/mlscript.tmLanguage.json"
    }]
  }
}
